            <Font size="25.0" />
         </font></TextField>
      <Button layoutX="221.0" layoutY="224.0" mnemonicParsing="false" onAction="#NumberGuess" text="Guess" />
      <Label fx:id="label" layoutX="111.0" layoutY="337.0" prefHeight="37.0" prefWidth="420.0" text="" textAlignment="CENTER">
         <font>
            <Font name="PT Sans" size="20.0" />
         </font>
      </Label>
      <Button layoutX="344.0" layoutY="224.0" mnemonicParsing="false" onAction="#ExitButton" text="Exit" />
      <Label layoutX="111.0" layoutY="41.0" prefHeight="73.0" prefWidth="378.0" text="Guess The Number From 1 To 100" textAlignment="CENTER">
         <font>
            <Font name="Tiro Tamil Italic" size="25.0" />
         </font>
      </Label>
      <Button layoutX="240.0" layoutY="287.0" mnemonicParsing="false" onAction="#ResetNum" text="Reset  The Number" />
